{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": ["standard", "prettier"],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 2021,
      "sourceType": "module"
    },
    "plugins": ["prettier", "@typescript-eslint"],
    "rules": {
      "prettier/prettier": "error",
      "spaced-comment": "off"
    }
  }
  